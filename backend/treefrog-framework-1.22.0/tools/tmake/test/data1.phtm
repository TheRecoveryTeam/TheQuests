	&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &lt;input type="radio" name=&quot;gakunen&quot; value=&quot;２年生&quot;&gt;２年生<br>
	&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &lt;input type="radio" name=&quot;gakunen&quot; value=&quot;３年生&quot;&gt;３年生<br>

	&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &lt;input type="radio" name=&quot;gakunen&quot; value=&quot;４年生&quot;&gt;４年生<br>
	&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &lt;input type="radio" name=&quot;gakunen&quot; value=&quot;５年生&quot;&gt;５年生<br>

	&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &lt;input type="radio" name=&quot;gakunen&quot; value=&quot;６年生&quot;&gt;６年生<br>
	&nbsp;&nbsp;&nbsp; &lt;/td><br>
	&nbsp; &lt;/tr><br>

	&nbsp; &lt;tr><br>
	&nbsp;&nbsp;&nbsp; &lt;td align="right">&lt;b&gt;好きな課目：&lt;/b&gt;&lt;/td><br>
	&nbsp;&nbsp;&nbsp; &lt;td><br>
	&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &lt;input type=&quot;checkbox&quot; name=&quot;kamoku&quot; value=&quot;国語&quot;&gt;国語<br>

	&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &lt;input type=&quot;checkbox&quot; name=&quot;kamoku&quot; value=&quot;英語&quot;&gt;英語<br>
