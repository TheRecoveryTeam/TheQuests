	&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &lt;input type=&quot;checkbox&quot; name=&quot;kamoku&quot; value=&quot;社会&quot;&gt;社会<br>

	&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &lt;input type=&quot;checkbox&quot; name=&quot;kamoku&quot; value=&quot;体育&quot;&gt;体育<br>
	&nbsp;&nbsp;&nbsp; &lt;/td><br>
	&nbsp; &lt;/tr&gt;<br>

	&nbsp; &lt;tr><br>
	&nbsp;&nbsp;&nbsp; &lt;td align="right">&lt;b&gt;宿題ファイル：&lt;/b&gt;&lt;/td><br>
	&nbsp;&nbsp;&nbsp; &lt;td>&lt;input type=&quot;file&quot; name=&quot;syukudai&quot;&gt;&lt;/td><br>

	&nbsp; &lt;/tr><br>
	&nbsp; &lt;tr><br>
	&nbsp;&nbsp;&nbsp; &lt;td&gt;&lt;/td><br>
	&nbsp;&nbsp;&nbsp; &lt;td><br>

	&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &lt;input type="submit" value="送信"><br>
	&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &lt;input type="reset" value="リセット"><br>
	&nbsp;&nbsp;&nbsp; &lt;/td><br>
	&nbsp; &lt;/tr><br>
	&lt;/table><br>

	&lt;/form>
